<?xml version="1.0" encoding="UTF-8"?>
<storage>
	<element name="Branding">
		<element name="Info">
			<attribute name="appName" value="openTrans" />
			<attribute name="appVersion" value="0.1.0" />
			<attribute name="author" value="Tomasz JÄ™drzejewski" />
			<attribute name="website" value="http://github.com/zyxist/opentrans" />
			<attribute name="copyright" value="Invenzzia Group 2012" />
			<attribute name="license" value="GNU General Public License 3" />
			<attribute name="promoImage" value="images/splash.gif" />
		</element>
		<element name="Splash">
			<attribute name="imagePath" value="images/splash.gif" />
		</element>
	</element>
	<element name="Cards">
		<element name="editor">
			<attribute name="ancestor" value="NULL" />
		</element>
		<element name="explorer">
			<attribute name="ancestor" value="editor" />
			<attribute name="orientation" value="right" />
			<attribute name="width" value="250" />
		</element>
		<element name="properties">
			<attribute name="ancestor" value="explorer" />
			<attribute name="orientation" value="top" />
			<attribute name="width" value="550" />
		</element>
		<element name="info">
			<attribute name="ancestor" value="editor" />
			<attribute name="orientation" value="top" />
			<attribute name="width" value="600" />
		</element>
	</element>
	<element name="Help">
		<element name="index"><attribute name="url" value="./docs/index.html" /></element>
	</element>
	<element name="Menu">
		<element name="File" keyStroke="Alt+F">
			<element name="New project" keyStroke="Ctrl+N">
				<attr name="action" value="newProject" />
			</element>
		</element>
	</element>
	<element name="Welcome">
		<element name="gradient">
			<attr name="startColor" value="0xFFFFFF" />
			<attr name="endColor" value="0xFFCCCC" />
		</element>
		<element name="text" color="0x550000" x="50" y="50" size="16">
			<attr name="text" value="Welcome to OpenTrans" />
		</element>
		<element name="link" color="0x550000" x="50" y="150" size="16">
			<attr name="text" value="Visit OpenTrans website" />
			<attr name="url" value="http://gihtub.com/zyxist/opentrans" />
		</element>
	</element>
</storage>
